{
	"$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true,
			"style": {
				"noParameterAssign": "off",
				"noNonNullAssertion": "off"
			}
		}
	},
	"files": {
		"ignore": ["dist", "package.json", "src/jsonc.ts"]
	}
}
